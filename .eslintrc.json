{
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
	"env": {
		"browser": true,
    "es6": true,
    "worker": true
	},
	"rules": {
		"no-console": "off",
		"no-extra-semi": "warn",
    "semi": "warn"
	},
	"extends": "eslint:recommended",
  "globals": {
    "L": true,
    "map": true
  }
}
